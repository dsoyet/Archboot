# Created by Tobias Powalowski <tpowa@archlinux.org>

install ()
{
    MODULES=""
    BINARIES="kexec"
    FILES=""
    SCRIPT=""
    add_file "/etc/rc.d/kexec"
    add_file "/etc/archboot/kexec/etc/conf.d/kexec" "/etc/conf.d/kexec"
}

help ()
{
cat<<HELPEOF
  This hook includes kexec on an arch boot image.
HELPEOF
} 
