# Created by Tobias Powalowski <tpowa@archlinux.org>

install ()
{
    MODULES=""
    BINARIES="dmraid"
    FILES=""
    SCRIPT="arch_dmraid"
    add_dir "/var/lock/dmraid"

}

help ()
{
cat<<HELPEOF
  This hook includes dmraid on an arch boot image.
  It depends on arch_devicemapper hook.
HELPEOF
} 
