# Created by Tobias Powalowski <tpowa@archlinux.org>

build ()
{
    MODULES=""
    BINARIES="/sbin/kexec"
    FILES=""
    SCRIPT=""
    add_file "/etc/rc.d/kexec"
    add_file "/usr/share/archboot/kexec/etc/conf.d/kexec" "/etc/conf.d/kexec"
}

help ()
{
cat<<HELPEOF
  This hook includes kexec on an arch boot image.
HELPEOF
} 
