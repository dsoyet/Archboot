build ()
{
    MODULES=""
    BINARIES="pptp"
    FILES=""
    SCRIPT=""
    add_file "/usr/share/archboot/pptpclient/etc/ppp/options.pptp" "/etc/ppp/options.pptp"

}

help ()
{
cat<<HELPEOF
  This hook includes pptpclient on an arch boot image.
HELPEOF
} 
