# Created by Tobias Powalowski <tpowa@archlinux.org>

install ()
{
    MODULES=""
    BINARIES=""
    FILES=""
    SCRIPT=""
    [ -d "/lib/modules/$KERNELVERSION/kernel/updates" ] && add_full_dir "/lib/modules/$KERNELVERSION/kernel/updates"
}

help ()
{
cat<<HELPEOF
  This hook includes updates kernel module dir  on an arch boot image.
HELPEOF
}
