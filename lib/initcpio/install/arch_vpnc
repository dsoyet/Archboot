# Created by Tobias Powalowski <tpowa@archlinux.org>

install ()
{
    MODULES=""
    BINARIES="pcf2vpnc vpnc vpnc-disconnect"
    FILES=""
    SCRIPT=""
    add_file "/etc/rc.d/vpnc"
    add_file "/usr/share/archboot/vpnc/etc/vpnc/default.conf" "/etc/vpnc/default.conf"
    add_file "/usr/share/archboot/vpnc/etc/vpnc/vpnc-script" "/etc/vpnc/vpnc-script"

}

help ()
{
cat<<HELPEOF
  This hook includes vpnc on an arch boot image.
HELPEOF
} 
