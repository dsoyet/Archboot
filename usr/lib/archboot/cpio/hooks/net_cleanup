_run ()
{
    _APPS="arping clockdiff tracepath ssh arpd \
      genl ifstat ctstat rtstat lnstat nstat rtacct rtmon ss tc \
      bridge dcb devlink rdma tipc vdpa ethtool elinks brctl"
    for i in ${_APPS}; do
        rm /usr/bin/"${i}"
    done
}

# vim: set ft=sh ts=4 sw=4 et:
